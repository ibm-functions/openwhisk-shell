{
  "name": "@ibm-functions/shell",
  "version": "1.4.2",
  "description": "An Electron-based shell for Apache OpenWhisk",
  "homepage": "https://github.com/ibm-functions/shell",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "postinstall": "./bin/seticon.sh && cd plugins && npm install && cd .. && npm run build && npm run compile",
    "build": "node ./bin/build.js",
    "compile": "cd ../dist && npm install && ./compile.js",
    "test": "cd ../tests && npm run test"
  },
  "repository": "https://github.com/ibm-functions/shell",
  "keywords": [
    "OpenWhisk",
    "CLI",
    "UI",
    "serverless"
  ],
  "author": {
    "name": "Nick Mitchell",
    "email": "nickm@us.ibm.com"
  },
  "license": "Apache-2.0",
  "bugs": "https://github.com/ibm-functions/shell",
  "devDependencies": {
    "electron": "~1.8.4",
    "nyc": "^11.3.0"
  },
  "dependencies": {
    "adm-zip": "^0.4.7",
    "colors": "^1.2.1",
    "columnify": "^1.5.4",
    "debug": "^3.1.0",
    "about-window": "^1.11.0",
    "electron-editor-context-menu": "^1.1.1",
    "es6-promise-pool": "^2.5.0",
    "expand-home-dir": "0.0.3",
    "inquirer": "^3.3.0",
    "js-beautify": "1.7.5",
    "jsome": "^2.5.0",
    "opn": "^5.3.0",
    "pretty-ms": "^3.1.0",
    "yargs-parser": "^9.0.2"
  },
  "optionalDependencies": {
    "fileicon": "^0.2.2"
  }
